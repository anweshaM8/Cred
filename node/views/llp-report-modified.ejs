<% var email_config = Config('application').email %>
   
    <div style="padding: 50px 0 20px 0;"></div>
    
    <table cellspacing="0" cellpadding="0" style="border: 0;width: 100%;margin: 0 auto;">
        <tr>
            <td style="font-family: Arial; font-size: 18.75pt;">
                <table width="100%" cellspacing="0" cellpadding="0">
                    <tr>
                        <%if (details.llp.legal_name) { %>
                            <td style="font-family: Arial; font-size: 18.75pt; color: #4d3d33; font-weight: 700; margin-bottom: 20px;"><%= details.llp.legal_name  %></td>
                        <% } %>
                        <td style="object-position: right;">
                            <img src="<%= email_config.logo %>" alt="Logo" style="width: 139px;height: 24px; padding-left: 500px;display: none;"/>
                        </td>
                    </tr>
                   
                </table>
            </td>
        </tr>
        <tr><td style="height: 20px;"></td></tr>
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                Key Information
            </td>
        </tr>
        <tr>
            <td style="font-family: Arial; font-size: 7.5pt;">
                <table width="100%" cellspacing="0" cellpadding="0">
                    <%if (details.llp.registered_address) { %>
                    <tr>
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 5px; width: 170px;">Registered Address</td>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 5px;">
                            <div><%if (details.llp.registered_address.address_line1) { %><%= details.llp.registered_address.address_line1 %><% } %></div>
                            <div><%if (details.llp.registered_address.address_line2) { %><%= details.llp.registered_address.address_line2 %><% } %></div>
                            <div><%if (details.llp.registered_address.city) { %><%= details.llp.registered_address.city %><% } %></div>
                            <div><%if (details.llp.registered_address.pincode) { %><%= details.llp.registered_address.pincode %><% } %></div>
                            <div><%if (details.llp.registered_address.state) { %><%= details.llp.registered_address.state %><% } %></div>
                        </td>
                    </tr>
                    <% } %>
                    <%if (details.llp.business_address) { %>
                    <tr>
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Business Address</td>
                        <td style="border-bottom: 1px solid #d1d1d1; padding:5px;">
                            <div><%if (details.llp.business_address.address_line1) { %><%= details.llp.business_address.address_line1 %><% } %></div>
                            <div><%if (details.llp.business_address.address_line2) { %><%= details.llp.business_address.address_line2 %><% } %></div>
                            <div><%if (details.llp.business_address.city) { %><%= details.llp.business_address.city %><% } %></div>
                            <div><%if (details.llp.business_address.pincode) { %><%= details.llp.business_address.pincode %><% } %></div>
                            <div><%if (details.llp.business_address.state) { %><%= details.llp.business_address.state %><% } %></div>
                        </td>
                    </tr>
                    <% } %>
                    <%if (details.llp.incorporation_date) { %>
                    <tr>
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Incorporation date</td>
                        <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= new Date(details.llp.incorporation_date).toLocaleDateString("en-US", { year: 'numeric', month: 'long', day: 'numeric' }) %></td>
                    </tr>
                    <% } %>
                    <%if (details.llp.classification) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Type of Entity</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.classification %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.website) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Website</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.website %></td>
                        </tr>
                    <% } %>
                    <%if (details.contact_details.email) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Email</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.contact_details.email[0].emailId %></td>
                        </tr>
                    <% } %>
                    <%if (details.contact_details.phone && details.contact_details.phone.length>0) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Phone</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.contact_details.phone[0].phoneNumber %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.llpin) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">LLPIN</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.llpin %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.pan) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">PAN</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.pan %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.total_obligation_of_contribution) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Total Obligation of
                                Contribution (INR, million)</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= (details.llp.total_obligation_of_contribution/1000000).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.total_contribution_received) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Total Contribution
                                Received (INR, million)</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= (details.llp.total_contribution_received/1000000).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.sum_of_charges) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">Sum of Charges (INR, million)</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= (details.llp.sum_of_charges/1000000).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.efiling_status) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">E-Filing Status</td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.efiling_status %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.last_annual_returns_filed_date) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;">FYED for which Annual
                                Report Filed
                                </td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= new Date(details.llp.last_annual_returns_filed_date).toLocaleDateString("en-US", { year: 'numeric', month: 'long', day: 'numeric' }) %></td>
                        </tr>
                    <% } %>
                    <%if (details.llp.lei) { %>
                        <tr>
                            <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding:5px; width: 170px;"> LEI
                                No
                                </td>
                            <td style="border-bottom: 1px solid #d1d1d1; padding:5px;"><%= details.llp.lei.number %></td>
                        </tr>
                    <% } %>
                </table>
            </td>
        </tr>
        <tr><td> <div style="page-break-after: always;"></div></td></tr>
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                About The LLP
            </td>
        </tr>
        <%if (details.description.desc_thousand_char) { %>
        <tr>
            <td style="font-family: Arial; border-bottom: 1px solid #d1d1d1; padding: 1px;font-size: 7.5pt;text-align: justify;">
                <%= details.description.desc_thousand_char %>
            </td>
        </tr>
        <% } %>
        <tr><td style="height: 20px;"></td></tr>
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                Industry And Segment(s)
            </td>
        </tr>
        <tr>
            <td style="font-family: Arial; font-size: 7.5pt;text-align: justify;">
                <table width="100%" cellspacing="0" cellpadding="0">
                    <%if (details.industry_segments) { %>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; width: 170px;font-size: 7.5pt;"><%= details.industry_segments[0].industry %></td>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;font-size: 7.5pt;">
                            <%= details.industry_segments[0].segments[0] %>
                        </td>
                    </tr>
                    <% } %>
                   
                </table>
            </td>
        </tr>
        <!-- <tr><td> <div style="page-break-after: always;"></div></td></tr> -->
        <tr><td style="height: 20px;"></td></tr>
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                Principal Business Activities  <%if (details.principal_business_activities && details.principal_business_activities.length>0) { %><%= details.principal_business_activities[0].year %><% } %>
            </td>
        </tr>
        <%if (details.principal_business_activities && details.principal_business_activities.length>0) { %>
        <tr>
            <td style="font-family: Arial; font-size: 7.5pt;text-align: justify;">
                <table width="100%" cellspacing="0" cellpadding="0">
                    <%if (details.principal_business_activities) { %> 
                    <tr>
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; width: 170px;">Business Classification</td>
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px;">
                            Description of Business Activity
                        </td>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; width: 170px;"><%= details.principal_business_activities[0].business_classification %></td>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;">
                            <%= details.principal_business_activities[0].principal_business_activities %>
                        </td>
                    </tr>
                    <% } %>
                   
                </table>
            </td>
        </tr>
        <% }else{ %> 
            <tr>
                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; font-size: 7.5pt;text-align: justify;">                 
                    There is no Principal Business Activities              
                </td>
            </tr>
        <% } %>
        <!-- <tr><td> <div style="page-break-after: always;"></div></td></tr> -->
        <tr><td style="height: 20px;"></td></tr>
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                Name History
            </td>
        </tr>
        <%if (details.name_history.length>0) { %>

            
            <tr>
                <td style="font-family: Arial; font-size: 7.5pt;text-align: left;">
                    <table width="100%" cellspacing="0" cellpadding="0">
                            <tr>
                                <td style="font-family: Arial;font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; width: 170px;">Main Activity
                                    Name</td>
                                <td style="font-family: Arial;font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px;">
                                    Till Date
                                </td>
                            </tr>
                            <%for(var j=0; j<details.name_history.length; j++) {  %>
                                <tr>
                                    <td style="font-family: Arial;border-bottom: 1px solid #d1d1d1; padding: 1px; width: 170px;"><%= details.name_history[j].name %></td>
                                    <td style="font-family: Arial;border-bottom: 1px solid #d1d1d1; padding: 1px;">
                                        <%= new Date(details.name_history[j].date).toLocaleDateString("en-US", { year: 'numeric', month: 'long', day: 'numeric' }) %>
                                    </td>
                                </tr>
                            <% } %>
                        
                    </table>
                </td>
            </tr>

        <% }else{ %> 
            <tr>
                <td style="font-family: Arial;border-bottom: 1px solid #d1d1d1; padding: 8px 15px 8px 0; font-size: 7.5pt;text-align: justify;">                 
                    This corporate has not had any name change as per our records.              
                </td>
            </tr>
        <% } %>
    </table>

    <!-- table no 1 -->
    <tr><td> <div style="page-break-after: always;"></div></td></tr>

    <!-- table no 2 -->
    <table cellspacing="0" cellpadding="0" style="border: 0;width: 100%;margin: 0 auto;">
      
        <tr>
            <td style="font-family: Arial; padding: 8px 15px 8px 0; background-color: #FFF; color: #FF5000;border-bottom: 1px solid #d1d1d1; font-weight: bold; font-size: 9pt;">
                Statement of Accounts
            </td>
        </tr>
        <%if (details.financials1.length>0) { %>
        <tr>
            <td style="font-family: Arial; font-size: 7.5pt;">
                <table width="100%" cellspacing="0" cellpadding="0">
                    <tr style="background-color: #f7f7f7;">
                        <td style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; width: 350px;">Statement of Assets and Liabilities (INR, million)</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #D1D1D1; padding: 3px;font-size: 6pt;"><%= new Date(details.financials1[j].year).toLocaleDateString("en-US", { year: 'numeric', month: 'long', day: 'numeric' }) %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    
                    <tr>
                        <td colspan="4" style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; width: 350px;">Contribution and Liabilities</td>
                    </tr>
                    <tr>
                        <td colspan="4" style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; ">Partner's Funds</td>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Contribution Received</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.contribution_received/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Reserves and Surplus</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.reserves_and_surplus/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td colspan="4" style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; ">Liabilities</td>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Secured Loans</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.secured_loan/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Unsecured Loans</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.unsecured_loan/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Short Term Borrowing</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.short_term_borrowing/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Creditor / Trade Payables - Advance from Customers</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.trade_payables/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Other Liabilities</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.other_liabilities/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td colspan="4" style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; ">Provisions</td>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">for Taxation</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.provisions_for_taxation/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">for Contingencies</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.provisions_for_contingencies/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">for Insurance</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.provisions_for_insurance/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Other Provisions</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.liabilities.other_provisions/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
                    <tr>
                        <td style="font-weight:bold; border-bottom: 1px solid #d1d1d1; padding: 1px; ">Total</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td  style="font-weight:bold; border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= details.financials1[j].statement_of_assets_and_liabilities.subTotals.given_liabilities_total!=null?(details.financials1[j].statement_of_assets_and_liabilities.subTotals.given_liabilities_total/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","):"" %></td>
                                
                            <% } %>
                        <% } %>
                       
                    </tr>
                    <tr>
                        <td colspan="4" style="font-weight: bold; border-bottom: 1px solid #d1d1d1; padding: 1px; width: 350px;">Assets</td>
                    </tr>
                    <tr>
                        <td style="border-bottom: 1px solid #d1d1d1; padding: 1px; ">Gross Fixed Assets</td>
                        <%if (details.financials1.length>0) { %>
                            <%for(var j=0; j<3; j++) {  %>
                                <td style="border-bottom: 1px solid #d1d1d1; padding: 1px;"><%= (details.financials1[j].statement_of_assets_and_liabilities.assets.gross_fixed_assets/1000000).toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %></td>
                                
                            <% } %>
                        <% } %>
                    </tr>
